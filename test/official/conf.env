CSPType[1]=aws
NumRegion[1]=8

CSPType[2]=alibaba
NumRegion[2]=1

CSPType[3]=gcp
NumRegion[3]=3

CSPType[4]=azure
NumRegion[4]=1

CSPType[5]=mock
NumRegion[5]=1

